<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.net.openid">
	<class name="CredentialHandler" table="openid_credential_handler">
		<id name="id" unsaved-value="null" type="java.lang.String"
			length="32" column="handler_id">
			<generator class="assigned" />
		</id>
		<property name="name" column="handler_name" not-null="true" />
		<property name="description" column="handler_description" />
		<property name="className" column="handler_classname"
			not-null="true" />
	</class>
</hibernate-mapping>
