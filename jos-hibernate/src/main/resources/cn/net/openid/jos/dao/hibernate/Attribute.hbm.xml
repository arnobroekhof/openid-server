<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.net.openid.jos.domain">
	<class name="Attribute" table="openid_attribute">
		<id name="id" unsaved-value="null" type="java.lang.String"
			length="32" column="attribute_id">
			<generator class="uuid" />
		</id>
		<property name="alias" length="32" column="attribute_alias" />
	</class>
</hibernate-mapping>
