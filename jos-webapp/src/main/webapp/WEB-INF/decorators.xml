<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/view/velocity/decorators">
	<excludes>
		<pattern>/*.ico</pattern>
		<pattern>*.css</pattern>
		<pattern>/*.png</pattern>
		<pattern>/*.gif</pattern>
		<pattern>/robots.txt</pattern>
		<pattern>/domain-configurator</pattern>
	</excludes>
	<decorator name="main" page="main.vm">
		<pattern>/*</pattern>
	</decorator>
</decorators>